<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Foros" table="foros">
    <id name="id" type="integer" column="id"/>
    <field name="titulo" type="string" column="titulo" length="255" nullable="false"/>
    <field name="created" type="datetime" column="created" nullable="false"/>
    <field name="modified" type="datetime" column="modified" nullable="false"/>
    <one-to-one field="tema" target-entity="Temas">
      <join-columns>
        <join-column name="tema_id" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
    <many-to-one field="uduario" target-entity="Usuarios">
      <join-columns>
        <join-column name="uduario_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="carrera" target-entity="Carreras">
      <join-columns>
        <join-column name="carrera_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
