<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Informacion" table="informacion">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="informacionTitulo" type="string" column="informacion_titulo" length="250" nullable="true"/>
    <field name="informacionImagen" type="string" column="informacion_imagen" length="100" nullable="true"/>
    <field name="informacionLink" type="string" column="informacion_link" length="100" nullable="true"/>
    <field name="informacionEstado" type="boolean" column="informacion_estado" nullable="true"/>
    <field name="informacionDestino" type="string" column="informacion_destino" length="45" nullable="true"/>
    <field name="created" type="datetime" column="created" nullable="true"/>
    <field name="modified" type="datetime" column="modified" nullable="true"/>
  </entity>
</doctrine-mapping>
