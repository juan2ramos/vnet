<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ColegiosPerfiles" table="colegios_perfiles">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="perfil" target-entity="Perfiles">
      <join-columns>
        <join-column name="perfil_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="colegio" target-entity="Colegios">
      <join-columns>
        <join-column name="colegio_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
