<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="PermisosProductos" table="permisos_productos">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="producto" target-entity="Productos">
      <join-columns>
        <join-column name="producto_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="permiso" target-entity="Permisos">
      <join-columns>
        <join-column name="permiso_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
