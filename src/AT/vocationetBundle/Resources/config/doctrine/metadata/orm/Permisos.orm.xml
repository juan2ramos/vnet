<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Permisos" table="permisos">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="permisoscol" type="string" column="permisoscol" length="45" nullable="false"/>
    <field name="nombre" type="string" column="nombre" length="60" nullable="false"/>
    <field name="descripcion" type="text" column="descripcion" nullable="true"/>
    <field name="permisoRoutes" type="text" column="permiso_routes" nullable="true"/>
    <many-to-one field="rol" target-entity="Roles">
      <join-columns>
        <join-column name="rol_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
