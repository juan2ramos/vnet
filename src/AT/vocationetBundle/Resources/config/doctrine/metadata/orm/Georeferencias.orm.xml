<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Georeferencias" table="georeferencias">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="georeferenciaPadreId" type="integer" column="georeferencia_padre_id" nullable="true"/>
    <field name="georeferenciaNombre" type="string" column="georeferencia_nombre" length="100" nullable="true"/>
    <field name="georeferenciaTipo" type="string" column="georeferencia_tipo" length="5" nullable="true"/>
    <field name="georeferenciaCodigo" type="string" column="georeferencia_codigo" length="45" nullable="true"/>
  </entity>
</doctrine-mapping>
